<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GymApp"
        tools:targetApi="31">
        <activity
            android:name=".ui.Entry.SetupActivity"
            android:exported="false"
            android:label="@string/title_activity_setup"
            android:theme="@style/Theme.GymApp.NoActionBar" />
        <activity
            android:name=".ui.Entry.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Entry.EntryActivity"
            android:exported="true"
            android:label="@string/title_activity_entry">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".MainActivity"
        android:exported="true"
        android:label="@string/app_name">
        <!-- Removed the intent-filter from MainActivity -->
    </activity>

</application>

    </manifest>
